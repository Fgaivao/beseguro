<p id="notice"><%= notice %></p>

<h1>Seguros</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Image</th>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @seguros.each do |seguro| %>
      <tr>
        <td><%= seguro.name %></td>
        <td><%= seguro.description %></td>
        <td><%= seguro.image %></td>
        <td><%= seguro.tipo %></td>
        <td><%= link_to 'Show', seguro %></td>
        <td><%= link_to 'Edit', edit_seguro_path(seguro) %></td>
        <td><%= link_to 'Destroy', seguro, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Seguro', new_seguro_path %>
